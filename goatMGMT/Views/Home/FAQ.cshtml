@model IEnumerable<goatMGMT.Models.FAQ>
    @{
    ViewBag.Title = "FAQ";
    Layout = @Url.Content("~/Views/Shared/_PublicLayout.cshtml");
    }
                      <div class="container" style="padding-top:55px;">
                          <h1 style="padding-bottom:15px;">Frequently Asked Questions</h1>
                          @foreach (var item in Model) {
                          <div class="panel panel-default">
                              <div class="panel-heading">
                                  <h3 class="panel-title">@Html.DisplayFor(modelItem => item.question)</h3>
                              </div>
                              <div class="panel-body">
                                  @Html.DisplayFor(modelItem => item.answer)
                              </div>
                          </div>
                              }
                      </div>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#active-faq").addClass("active");
            $("#layout-header").css({ "background-image": "url(/Content/imgs/aboutHeader.png)" });
        });
    </script>
